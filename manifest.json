{
  "manifest_version": 2,
  "name": "WinMan",
  "description": "Window Manager: name, switch and move tabs between windows",
  "version": "0.1.0",
  "icons": {
    "64": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icon.png"
    },
    "default_title": "WinMan",
    "default_popup": "popup.html",
    "browser_style": true
  }
}
