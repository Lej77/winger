{
  "manifest_version": 2,
  "name": "WinMan",
  "description": "Window Manager: name windows, switch windows and move tabs between windows",
  "version": "0.1.0",
  "icons": {
    "64": "icon.png"
  },
  "background": {
    "scripts": [
      "bg.metadata.js",
      "bg.browserop.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icon.png"
    },
    "default_title": "WinMan",
    "default_popup": "popup.html",
    "browser_style": true
  },
  "commands": {
    "_execute_browser_action": {
      "description": "Open the popup panel",
      "suggested_key": {
        "default": "Ctrl+E"
      }
    }
  }
}
