{
  "manifest_version": 2,
  "name": "Winger",
  "description": "Window Manager: Name windows, switch windows, move tabs between windows.",
  "version": "1.3",
  "icons": {
    "48": "icons/winger.svg",
    "96": "icons/winger.svg"
  },
  "permissions": [
    "sessions",
    "storage",
    "tabs",
    "menus"
  ],
  "background": {
    "page": "background/background.html"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/winger.svg",
      "96": "icons/winger.svg"
    },
    "default_popup": "popup/popup.html",
    "browser_style": true
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },
  "commands": {
    "_execute_browser_action": {
      "description": "Open the pop-up panel",
      "suggested_key": {
        "default": "Alt+W"
      }
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "winman@lionelw"
    }
  }
}
